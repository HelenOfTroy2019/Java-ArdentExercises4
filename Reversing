public class reversing {

	public static void main(String[] args) {
		int ten[] = new int[10];
		for(int i=0; i<10; i++) {
			ten[i] = (int)(Math.random()*10);
		}
		for(int i=0; i<10; i++) {
			System.out.print(ten[i]+" ");
		}
		for(int i=0; i<10; i++) {
			ten[i] = ten[9-i];
		}
		System.out.println("");
		for(int i=0; i<10; i++) {
			System.out.print(ten[i]+" ");
		}
	}

}
